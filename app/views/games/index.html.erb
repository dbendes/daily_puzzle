<%= render 'jumbotron' %>
<% if notice %>
  <p class="alert alert-success"><%= notice %></p>
<% end %>
<% if alert %>
  <p class="alert alert-danger"><%= alert %></p>
<% end %>

<div class="row">
  <% @games.each do |game| %>
    <%= render partial: 'gamebox', locals: {game: game} %>
  <% end %>
</div>

</div>
